{
  "name": "@cdm/database",
  "version": "0.0.1",
  "private": true,
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./client": "./src/client.ts"
  },
  "scripts": {
    "db:generate": "dotenv -e ../../.env -- prisma generate",
    "db:push": "dotenv -e ../../.env -- prisma db push",
    "db:migrate": "dotenv -e ../../.env -- prisma migrate dev",
    "db:reset": "dotenv -e ../../.env -- prisma migrate reset --force",
    "db:studio": "dotenv -e ../../.env -- prisma studio",
    "db:seed": "dotenv -e ../../.env -- npx tsx prisma/seed.ts",
    "build": "prisma generate",
    "lint": "eslint src/"
  },
  "prisma": {
    "seed": "npx tsx prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.9.0"
  },
  "devDependencies": {
    "@cdm/config": "workspace:*",
    "@types/node": "^22.0.0",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.39.2",
    "prisma": "^6.9.0",
    "tsx": "^4.21.0",
    "typescript": "^5.7.0"
  }
}